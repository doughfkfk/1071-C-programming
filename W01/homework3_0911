#include<stdio.h>
#include<math.h>
int main(){
	int a,b,c;
	double x1,x2;
	printf("請輸入a,b,c:");
	scanf("%d%d%d",&a,&b,&c);
	if(b*b-4*a*c>=0){
	x1=(-b+sqrt(b*b-4*a*c))/(2*a);
	x2=(-b-sqrt(b*b-4*a*c))/(2*a);}
	else{printf("此方程式無實根");}
	printf("For equation %d x^2 +%d x + %d =0,two roots are\nx1=%.1f\nx2=%.1f",a,b,c,x1,x2);
}
